{
  "name": "microrouter",
  "description": "ðŸš‰ A tiny and functional router for ZEIT's Micro",
  "version": "3.0.0",
  "main": "dist/index.js",
  "jsnext:main": "lib/index.js",
  "scripts": {
    "release": "np",
    "build": "rm -rf dist/* && babel lib --ignore *.test.js --out-dir dist --copy-files",
    "test": "xo && nyc ava",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": "git@github.com:pedronauck/micro-router.git",
  "author": "Pedro Nauck <pedronauck@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "micro": "^9.0.0",
    "url-pattern": "^1.0.3"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "coveralls": "^3.0.0",
    "np": "^2.16.0",
    "nyc": "^11.2.1",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "test-listen": "^1.0.2",
    "xo": "^0.18.2"
  },
  "xo": {
    "space": 2,
    "semicolon": false,
    "rules": {
      "curly": 0,
      "object-curly-spacing": 0,
      "unicorn/explicit-length-check": 0,
      "no-await-in-loop": 0
    }
  },
  "engines": {
    "node": ">=6.10.0"
  }
}
