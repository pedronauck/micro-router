{
  "name": "microrouter",
  "description": "ðŸš‰ A tiny and functional router for Zeit's Micro",
  "version": "2.2.0",
  "main": "dist/index.js",
  "jsnext:main": "lib/index.js",
  "scripts": {
    "build": "rm -rf dist/* && babel lib --ignore *.test.js --out-dir dist --copy-files",
    "test": "xo && nyc ava",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": "git@github.com:pedronauck/micro-router.git",
  "author": "Pedro Nauck <pedronauck@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "micro": "^7.3.3",
    "url-pattern": "^1.0.3"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "coveralls": "^2.11.16",
    "micro": "^7.0.6",
    "nyc": "^10.1.2",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
    "test-listen": "^1.0.1",
    "xo": "^0.17.1"
  },
  "xo": {
    "space": 2,
    "semicolon": false,
    "rules": {
      "curly": 0,
      "object-curly-spacing": 0,
      "unicorn/explicit-length-check": 0,
      "no-await-in-loop": 0
    }
  },
  "engines": {
    "node": ">=6.10.0"
  }
}
